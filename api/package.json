  "description": "API para atender las llamadas a un bot de Telegram",
  "main": "index.js",
  "scripts": {
    "migrate": "npm-run-all --parallel migrate:mongoose migrate:sequelize",
    "migrate:mongoose": "npx migrate-mongo up",
    "migrate:sequelize": "sequelize-cli db:migrate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --env-file=.env index.js",
    "dev": "node --env-file=.env --watch index.js"
  },
